    "ethers": "^6.4.0",
    "ethjs-abi": "^0.2.1",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.61.0",
    "prom-client": "^14.2.0",
    "sequelize": "^6.32.1",
    "sinon": "^15.2.0",
    "sqlite3": "^5.1.6",
    "web3": "4.0.3",
    "winston": "^3.10.0"
  },
  "devDependencies": {
