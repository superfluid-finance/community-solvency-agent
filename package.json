    "@truffle/contract": "^4.6.26",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "bip39": "^3.1.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "eth-block-tracker": "^7.1.0",
